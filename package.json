{
    "name": "bulletproof-react",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@headlessui/react": "^1.4.1",
        "@heroicons/react": "^1.0.4",
        "@hookform/resolvers": "^2.8.1",
        "@tailwindcss/typography": "^0.4.1",
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^12.1.2",
        "@testing-library/user-event": "^13.2.1",
        "@types/jest": "^27.0.2",
        "@types/node": "^16.10.3",
        "@types/react": "^17.0.27",
        "@types/react-dom": "^17.0.9",
        "@vitejs/plugin-react-refresh": "^1.3.6",
        "axios": "^0.22.0",
        "clsx": "^1.1.1",
        "dayjs": "^1.10.7",
        "dompurify": "^2.3.3",
        "history": "^5.0.1",
        "intersection-observer": "^0.12.0",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.21",
        "marked": "^3.0.4",
        "nanoid": "^3.1.28",
        "postcss": "^8.3.9",
        "postcss-cli": "^9.0.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-error-boundary": "^3.1.3",
        "react-flow-renderer": "^9.6.9",
        "react-helmet": "^6.1.0",
        "react-hook-form": "^7.17.1",
        "react-query": "^3.25.1",
        "react-query-auth": "^1.1.0",
        "react-router-dom": "^6.0.0-beta.0",
        "tailwindcss": "^2.2.16",
        "type-fest": "^2.3.4",
        "typescript": "^4.4.3",
        "vite": "^2.6.3",
        "vite-plugin-svgr": "^0.4.0",
        "vite-react-jsx": "^1.1.2",
        "web-vitals": "^2.1.0",
        "zod": "^3.9.8",
        "zustand": "^3.5.12"
    },
    "scripts": {
        "build-storybook": "build-storybook -s src/public",
        "build": "npm run tailwind:build && vite build",
        "cy:install": "cypress install",
        "cy:open": "cypress open",
        "cy:run": "cypress run",
        "format:check:prettier": "npm run format:prettier -- --list-different",
        "format:lint": "eslint --fix --ext .js,.ts,.tsx,.jsx ./src --ignore-path .gitignore",
        "format:prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|ts|tsx)\"",
        "format": "npm run format:prettier && npm run format:lint",
        "generate": "plop",
        "postinstall": "husky install",
        "pretest:e2e:run": "yarn build",
        "serve": "vite preview",
        "start:dev": "npm-run-all -p tailwind:watch start:vite",
        "start": "yarn start:vite",
        "storybook": "start-storybook -p 6006 -s src/public",
        "tailwind:build": "postcss ./src/assets/styles/index.css -o ./src/assets/styles/tailwind.css --env production",
        "tailwind:watch": "postcss ./src/assets/styles/index.css -o ./src/assets/styles/tailwind.css -w",
        "test:coverage": "CI=1 yarn test:jest -- --coverage",
        "test:e2e:dev": "start-server-and-test start:cli http://localhost:3000 cy:open",
        "test:e2e:run": "start-server-and-test serve http://localhost:3000 cy:run",
        "test:e2e": "is-ci \"test:e2e:run\" \"test:e2e:dev\"",
        "test:jest": "jest",
        "test:watch": "jest --watch ",
        "test": "is-ci \"test:coverage\" \"test:watch\"",
        "validate": "yarn format",
        "validate-and-build": "npm-run-all --parallel format:check:prettier lint build",
        "start:vite": "vite"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "resolutions": {
        "babel-loader": "8.1.0"
    },
    "devDependencies": {
        "@cypress/react": "^5.10.1",
        "@cypress/vite-dev-server": "^2.1.1",
        "@material-tailwind/react": "^0.3.4",
        "@mswjs/data": "^0.5.1",
        "@storybook/addon-actions": "^6.3.9",
        "@storybook/addon-essentials": "^6.3.9",
        "@storybook/addon-links": "^6.3.9",
        "@storybook/node-logger": "^6.3.9",
        "@storybook/preset-create-react-app": "^4.0.0",
        "@storybook/react": "^6.3.9",
        "@testing-library/cypress": "^8.0.1",
        "@testing-library/react-hooks": "^7.0.2",
        "@types/dompurify": "^2.3.1",
        "@types/faker": "^5.5.8",
        "@types/jsonwebtoken": "^8.5.5",
        "@types/lodash": "^4.14.175",
        "@types/marked": "^3.0.1",
        "@types/react-helmet": "^6.1.3",
        "autoprefixer": "^10",
        "babel-eslint": "^10.1.0",
        "cross-env": "^7.0.3",
        "cypress": "^8.6.0",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-config-react-app": "^6.0.0",
        "eslint-import-resolver-typescript": "^2.5.0",
        "eslint-plugin-cypress": "^2.12.1",
        "eslint-plugin-flowtype": "^6.1.0",
        "eslint-plugin-import": "^2.24.2",
        "eslint-plugin-jest-dom": "^3.9.2",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.26.1",
        "eslint-plugin-react-hooks": "^4.2.0",
        "eslint-plugin-sort-keys-fix": "^1.1.2",
        "eslint-plugin-testing-library": "^4.12.4",
        "faker": "^5.5.3",
        "husky": "^7.0.2",
        "is-ci": "^3.0.0",
        "is-ci-cli": "^2.2.0",
        "lint-staged": "^11.2.0",
        "msw": "^0.35.0",
        "npm-run-all": "^4.1.5",
        "plop": "^2.7.4",
        "prettier": "^2.4.1",
        "prettier-eslint": "^13.0.0",
        "prettier-eslint-cli": "^5.0.1",
        "react-test-renderer": "^17.0.2",
        "serve": "^12.0.1",
        "start-server-and-test": "^1.14.0"
    },
    "msw": {
        "workerDirectory": "public"
    },
    "jest": {
        "moduleNameMapper": {
            "^@/(.+)": "<rootDir>/src/$1"
        },
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}",
            "!src/**/*.d.ts",
            "!src/**/*.stories.{js,jsx,ts,tsx}",
            "!src/test/**/*.{js,jsx,ts,tsx}"
        ]
    },
    "eslintConfig": {
        "overrides": [
            {
                "files": [
                    "**/*.stories.*"
                ],
                "rules": {
                    "import/no-anonymous-default-export": "off"
                }
            }
        ]
    },
    "lint-staged": {
        "*.+(js|jsx)": [
            "yarn format"
        ]
    }
}
